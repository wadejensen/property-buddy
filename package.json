{
  "name": "hello-node",
  "version": "1.0.0",
  "description": "\"My first time playing with node in earnest.\"",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "start": "nodemon lib/index.js --exec babel-node --presets es2015,stage-2",
    "build": "babel lib -d dist --presets es2015,stage-2",
    "serve": "node dist/index.js",
    "debug": "nodemon lib/index.js --exec babel-node --presets es2015,stage-2 --debug-brk"
  },
  "author": "wadejensen",
  "license": "Apache-2.0",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "es6-promisify": "^5.0.0",
    "express": "^4.16.2",
    "http-server": "^0.10.0",
    "node-fetch": "^1.7.3",
    "nodemon": "^1.12.5",
    "xml2js": "^0.4.19"
  }
}
