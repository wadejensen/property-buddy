  "description": "\"Trying to get node and typescript working. Much easier than babel so far.\"",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "start": "nodemon lib/index.js --exec babel-node --presets es2015,stage-2",
